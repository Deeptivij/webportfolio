
<div class="col-md-12 col-xs-12 col-sm-12 col-lg-12 smalldiv">
<% if @pictures.any? %>

    
<% @pictures.in_groups_of(3) do |group| %>
              <% group.each do |p| %>
                <% if p %>
                  <div>
              <div class="col-md-6 col-xs-12 col-sm-6 col-lg-3 photoplace view view-first">
                <%= image_tag(p.image.url, class: 'media-object')%>
             </div>
                <% end %>
            </div>
        <% end %>
    <% end %>

 
<% end %>
<%= will_paginate @pictures %>
  </div>
